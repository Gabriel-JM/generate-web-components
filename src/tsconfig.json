{
  "compilerOptions": {
    "target": "es2017",
    "module": "ESNext",
    "lib": [
      "DOM",
      "ES2019"
    ],
    "baseUrl": "./",
    "paths": {
      "/*": ["*"]
    },
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "esModuleInterop": true
  }
}